{
  "version": 2,
  "builds": [
    {
      "src": "api/download.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/download-audio.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/download",
      "dest": "/api/download.js"
    },
    {
      "src": "/api/download-audio",
      "dest": "/api/download-audio.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
